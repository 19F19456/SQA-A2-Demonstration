public class main {
	public static void main(String[] args) {
		String[] arr = {"Hamed","Saif","Hilal","Mohammed"};
		System.out.println("Please enter Username");
		String UserName = System.in.toString();
		System.out.println("Please enter Password");
		String Password = System.in.toString();
		Boolean UserIsCorrect;
		Boolean PasswordIsCorrect;
		for(int i = 0; i < arr.length ; i++); {
			if(arr[i] == "Hamed" && Username == “Hamed”) {
				UserIsCorrect = true;
				break;}
			else
				i++;
		}
		switch (Password) {
		case "19F19456":
			PasswordIsCorrect = true;
			break;
		default:
			System.out.println("Password is incorrect");
			break;
		}
		if(PasswordIsCorrect = true && UserIsCorrect == true) {
			System.out.println("Username and password ACCEPTED");}
		else 
			System.out.println("Username and password UNACCEPTED");
	}
}
