#include <stdio.h>
#define ARR_SIZE 5
int main() {
    int a[ARR_SIZE]={1,2,3,4,5};
    int i,val,count=0;
    char temp;
    printf("Enter character 'P' to print array elements\n");
    scanf("%c",&temp);
    if (temp=='P')
    {
    switch (temp)
    {
    case 'P':
    for (i=0;i<ARR_SIZE;i++)
    {  
   printf("a[%d]:%d\n",count,a[i]);
   count++;
    }
  break;     
  default:
        printf("Invalid Key Entry\n");
    }
    }
    else{
        printf("Invalid Key Entry");
    }  }
